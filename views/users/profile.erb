
<div>
    <h3>sign up for alerts from <%= @current_user.username %>!</h3>

    <div id="log-out-div">

        <form id="log-out-button" action="/sessions" method="post">
            <input type="hidden" name="_method" value="delete">
            <input type="submit" value="Log Out">

        </form>
         &nbsp;

</div>
    </div>








<form class="create-list_entry" action="/api/list_entry" method="post">
    <input type="text" id="my-name" name="list_entry[name]" placeholder="...name">
    <input type="text" id="my-email" name="list_entry[email]" placeholder="...email">
    <input type="submit" value="Subscribe!">

</form>



<section id="list_entries-list">

</section>


<input type="hidden" id='api-token' value="<%= @current_user.token %>">
